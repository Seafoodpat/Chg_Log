<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Chq number</th>
      <th>Chq date</th>
      <th>Payee Name</th>
      <th>Category</th>
      <th>Deal</th>
      <th>Particular</th>
      <th>Currencies</th>
      <th>Amount</th>
      <th>Prepared</th>
      <th>Sign date</th>
      <th>Present date</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.chq_number %></td>
        <td><%= log.chq_date %></td>
        <td><%= log.payee_name %></td>
        <td><%= log.category %></td>
        <td><%= log.deal_id %></td>
        <td><%= log.particular %></td>
        <td><%= log.currencies %></td>
        <td><%= log.amount %></td>
        <td><%= log.prepared %></td>
        <td><%= log.sign_date %></td>
        <td><%= log.present_date %></td>
        <td><%= log.status %></td>
        <td><%= link_to 'Show', log %></td>
        <td><%= link_to 'Edit', edit_log_path(log) %></td>
        <td><%= link_to 'Destroy', log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


